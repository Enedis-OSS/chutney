[ req ]
default_bits       = 4096
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = v3_req

[ dn ]
C  = FR
ST = France
L  = Paris
O  = CHUTNEY TEST
OU = CLIENT CHUTNEY TEST
CN = client

[ v3_req ]
keyUsage = digitalSignature, keyEncipherment
extendedKeyUsage = clientAuth
